{
  "name": "cvs-svc-test-results",
  "version": "1.0.0",
  "description": "",
  "main": "src/handler.js",
  "scripts": {
    "start": "npm run eslint && BRANCH=local ./node_modules/serverless/bin/serverless offline start",
    "start:test": "npm run eslint && SLS_DEBUG=* BRANCH=local node_modules/gulp/bin/gulp.js start-serverless",
    "end:test": "node_modules/gulp/bin/gulp.js kill-serverless",
    "debug": "export SLS_DEBUG=* && BRANCH=local node --inspect ./node_modules/serverless/bin/serverless offline start",
    "test": "node_modules/.bin/mocha tests/**/*.unitTest.js",
    "test-i": "BRANCH=local node_modules/.bin/mocha tests/**/*.intTest.js",
    "dynamodb": "sls dynamodb start",
    "prepush": "BRANCH=local npm test && npm run eslint",
    "predeploy": "npm install && npm run prepush",
    "security-checks": "git secrets --scan && git log -p | scanrepo",
    "eslint": "eslint . --config=.eslintrc.json --fix",
    "sonar-scanner": "echo 'TODO'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvsa/cvs-svc-test-results.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dvsa/cvs-svc-test-results/issues"
  },
  "homepage": "https://github.com/dvsa/cvs-svc-test-results#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "date-fns": "^1.30.1",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^23.6.0",
    "gulp": "^4.0.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "serverless": "^1.32.0",
    "serverless-dynamodb-local": "0.2.35",
    "serverless-offline": "^3.27.0",
    "standard": "^12.0.1",
    "supertest": "^3.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "dependencies": {
    "aws-sdk": "^2.332.0",
    "joi": "^14.3.1",
    "uuid": "^3.3.2"
  }
