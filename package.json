{
  "name": "cvs-svc-test-results",
  "version": "1.0.0",
  "description": "",
  "main": "src/handler.js",
  "scripts": {
    "start": "npm run eslint && BRANCH=local ./node_modules/serverless/bin/serverless offline start",
    "start:test": "npm run eslint && SLS_DEBUG=* BRANCH=local node_modules/gulp/bin/gulp.js start-serverless",
    "end:test": "node_modules/gulp/bin/gulp.js kill-serverless",
    "debug": "export SLS_DEBUG=* && BRANCH=local node $NODE_DEBUG_OPTION ./node_modules/serverless/bin/serverless offline start",
    "test": "node_modules/.bin/mocha tests/**/*.unitTest.js",
    "test-i": "BRANCH=local node_modules/.bin/mocha tests/**/*.intTest.js",
    "dynamodb": "sls dynamodb start",
    "prepush": "BRANCH=local npm test && npm run eslint",
    "predeploy": "npm install && npm run prepush",
    "security-checks": "git secrets --scan && git log -p | scanrepo",
    "eslint": "eslint . --config=.eslintrc.json --fix",
    "sonar-scanner": "npm run eslint && npm run coverage && sonar-scanner",
    "coverage": "nyc mocha tests/unit/*.unitTest.js && nyc report --reporter=lcov",
    "audit-dep": "npm audit --json | audit-filter --nsp-config=.nsprc --audit=-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvsa/cvs-svc-test-results.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dvsa/cvs-svc-test-results/issues"
  },
  "homepage": "https://github.com/dvsa/cvs-svc-test-results#readme",
  "devDependencies": {
    "audit-filter": "^0.4.3",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^24.8.0",
    "gulp": "^4.0.2",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "serverless": "^1.42.3",
    "serverless-dynamodb-local": "^0.2.37",
    "serverless-offline": "^3.33.0",
    "serverless-plugin-tracing": "^2.0.0",
    "sinon": "^7.3.2",
    "standard": "^12.0.1",
    "supertest": "^3.4.2"
  },
  "dependencies": {
    "aws-sdk": "^2.456.0",
    "aws-xray-sdk": "^2.3.3",
    "date-fns": "^1.30.1",
    "joi": "^14.3.1",
    "lambda-warmer": "^1.1.2",
    "node-yaml": "^3.2.0",
    "path-parser": "^4.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
}
